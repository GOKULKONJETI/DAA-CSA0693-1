#include <stdio.h>
int main()
{
	int num,r,sum=0,temp;
	printf("enter a number:");
	scanf("%d",&num);
	
	for(num=temp;num!=0;num=num/10)
	{
		r = num % 10;
		sum = sum +(r*r*r);
	}
	if(sum==temp)
	    printf("given number is an armstrong number\n",temp);
	else
	    printf("given number is not an armstrong number\n",temp);
}
